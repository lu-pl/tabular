@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm#> .

{% set cnt = fn["cnt"]() %}

<https://core.clscor.io/entity/type/acronym> crm:P2i_is_type_of
<https://{{ data["id"] | lower}}.clscor.io/entity/corpus/title/acronym> .

<https://{{ data["id"] }}.clscor.io/entity/corpus> crm:P1_is_identified_by
    <https://rem.clscor.io/entity/corpus/title/acronym> .

<https://{{ data["id"] }}.clscor.io/entity/corpus/title/acronym> a crm:E41_Appellation ;
    crm:P1i_identifies <https://{{ data["corpusAcronym"] | lower }}.clscor.io/entity/corpus> ;
    crm:P2_has_type <https://core.clscor.io/entity/type/acronym> ;
    rdf:value '{{ data["full_title"] }} {{ cnt }}' .
